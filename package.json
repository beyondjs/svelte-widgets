{
  "name": "@beyond-js/svelte-widgets",
  "version": "0.1.2",
  "description": "Create web projects as standalone microfrontends with Svelte.",
  "license": "MIT",
  "modules": {
    "path": "./"
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/widgets"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
